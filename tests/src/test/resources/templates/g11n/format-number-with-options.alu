@library (c: http://aluminumproject.googlecode.com/core)
@library (g: http://aluminumproject.googlecode.com/g11n)

@g.format number (value: ${number}, grouping: false, c.variable name: no grouping)
@g.format number (value: ${number}, maximum fraction digits: 0, c.variable name: no fraction digits)
@g.format number (value: ${number}, minimum fraction digits: 2, c.variable name: two fraction digits)
@g.format number (value: ${number}, maximum integer digits: 1, c.variable name: one integer digit)
@g.format number (value: ${number}, minimum integer digits: 7, c.variable name: seven integer digits)